{
  "ConnectionStrings": {
    "DefaultConnection": "#{POSTGRES_CONNECTION_STRING}#",
    "PostgresConnection": "#{POSTGRES_CONNECTION_STRING}#"
  },
  "DatabaseType": "postgres",
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "AllowRegistration": false,
  "SeedAdminUser": false,
  "AdminUser": {
    "UserName": "#{ADMIN_USERNAME}#",
    "Password": "#{ADMIN_PASSWORD}#"
  },
  "Cors": {
    "AllowedOrigins": [
      "#{APP_URL}#"
    ]
  },
  "SpaSettings": {
    "DefaultPage": "browser/index.html"
  },
  "Email": {
    "ApiKey": "#{EMAIL_API_KEY}#",
    "ApiSecret": "#{EMAIL_API_SECRET}#",
    "To": "#{EMAIL_TO}#"
  }
}
